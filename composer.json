{
  "name": "tourze/shadowsocks-config-php",
  "description": "Shadowsocks Origin",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-filter": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0",
    "shipmonk/composer-dependency-analyser": "^1.8"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "Shadowsocks\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Shadowsocks\\Config\\Tests\\": "tests"
    }
  }
}